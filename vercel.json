{
  "version": 2,
  "builds": [
    {
      "src": "/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/((?!api/|assets/).*)",
      "dest": "/index.html"
    }
  ]
}
